    {
        "id": "fn_extract",
        "type": "function",
        "z": "29d6d8b64f0b4257",
        "name": "extrair",
        "func": "let cep = null;\nif(msg.req && msg.req.params && msg.req.params.cep){ cep = msg.req.params.cep; }\nif(msg.req && msg.req.query && msg.req.query.cep){ cep = msg.req.query.cep; }\nif(cep){ cep = cep.replace(/\\D/g,\"\"); }\nif(!cep || cep.length!==8){\n    msg.error = true;\n    msg.message = \"CEP inválido.\";\n    delete msg.display;\n    return msg;\n}\nmsg.url = \"https://brasilapi.com.br/api/cep/v2/\" + cep;\nreturn msg;",
        "outputs": 1,
