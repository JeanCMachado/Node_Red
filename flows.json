    {
        "id": "fn_process",
        "type": "function",
        "z": "29d6d8b64f0b4257",
        "name": "processar",
        "func": "if(msg.error){\n    delete msg.display;\n    return msg;\n}\nconst api = msg.payload;\nif(!api || typeof api !== \"object\"){\n    msg.error = true;\n    msg.message = \"Erro API.\";\n    return msg;\n}\nmsg.error = false;\nmsg.display = {\n    cep: api.cep || \"\",\n    street: api.street || api.logradouro || \"\",\n    neighborhood: api.neighborhood || api.bairro || \"\",\n    city: api.city || api.localidade || \"\",\n    state: api.state || api.uf || \"\"\n};\nreturn msg;",
